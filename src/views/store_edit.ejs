<% include partials/_header %>

<div class="container mt-5">
  <div class="row">
    <div class="col-md-5 offset-md-3 card">
      <div class="card-body">
        <form action="/update/<%= data.id %>" method="POST">
          <div class="form-group">
            <input type="text" name="manager_staff_id" value="<%= data.name%>" placeholder="Manager ID" class="form-control" autofocus>
          </div>
          <div class="form-group">
            <input type="text" name="address_id" value="<%= data.address%>" placeholder="Address ID" class="form-control">
          </div>
          <button type="submit" class="btn btn-primary">
            Update store
          </button>
        </form>
      </div>
    </div>
  </div>
</div>

<% include partials/_footer %>
